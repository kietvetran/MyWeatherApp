{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "js/*": ["./js/*"]
        },
        "target": "es2020",
        "module": "commonjs",
        "allowJs": true,
        "jsx": "react-native",
        "noEmit": true,
        "isolatedModules": true,
        "strict": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": ["es2019"],
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "extends": "expo/tsconfig.base",
}
